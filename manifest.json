{
  "manifest_version": 2,
  "name": "DarkTube",
  "version": "0.3.0",
  "description": "Forces YouTube to use dark theme.",
  "developer": {
    "name": "Ezerous",
    "url": "https://github.com/Ezerous"
  },
  "icons": {
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["dark-theme.css"]
    }
  ]
}
